ter seeding db => allow read, write: if request.auth.uid != null",["200","201"],"/home/mantoss/netflix-clone/src/App.js",["202"],"import React from 'react';\nimport {Home, Browse, Signin, Signup} from './pages';\nimport * as ROUTES from './constants/routes';\nimport { BrowserRouter as Router, Switch, Route } from 'react-router-dom';\n// eslint-disable-next-line\nimport './App.css';\nimport { useAuthListener } from './hooks';\nimport { IsUserRedirect, ProtectedRoute } from './helpers/routes';\n\nfunction App() {\n  const { user } = useAuthListener();\n\n  return (\n    <Router>\n      <Switch>\n        <IsUserRedirect user={user} loggedInPath={ROUTES.BROWSE} path={ROUTES.SIGN_IN}>\n            <Signin />\n        </IsUserRedirect>\n\n        <IsUserRedirect user={user} loggedInPath={ROUTES.BROWSE} path={ROUTES.SIGN_UP}>\n            <Signup />\n        </IsUserRedirect>\n\n        <ProtectedRoute user={user} path={ROUTES.BROWSE}>\n            <Browse />\n        </ProtectedRoute>\n\n        <IsUserRedirect user={user} loggedInPath={ROUTES.BROWSE} path={ROUTES.HOME}>\n            <Home />\n        </IsUserRedirect>\n      </Switch>\n    </Router>\n  );\n}\n\nexport default App;\n","/home/mantoss/netflix-clone/src/reportWebVitals.js",[],["203","204"],"/home/mantoss/netflix-clone/src/pages/Home.js",[],"/home/mantoss/netflix-clone/src/components/jumbotron/index.js",[],"/home/mantoss/netflix-clone/src/components/jumbotron/styles/jumbotron.js",[],"/home/mantoss/netflix-clone/src/containers/Jumbotron.js",[],"/home/mantoss/netflix-clone/src/components/index.js",[],"/home/mantoss/netflix-clone/src/globalStyles.js",[],"/home/mantoss/netflix-clone/src/components/footer/index.js",[],"/home/mantoss/netflix-clone/src/components/footer/styles/footer.js",[],"/home/mantoss/netflix-clone/src/containers/Footer.js",[],"/home/mantoss/netflix-clone/src/components/accordion/index.js",[],"/home/mantoss/netflix-clone/src/components/accordion/styles/accordion.js",[],"/home/mantoss/netflix-clone/src/containers/Faqs.js",[],"/home/mantoss/netflix-clone/src/components/opt-form/index.js",[],"/home/mantoss/netflix-clone/src/components/opt-form/styles/opt-form.js",[],"/home/mantoss/netflix-clone/src/constants/routes.js",[],"/home/mantoss/netflix-clone/src/components/header/index.js",[],"/home/mantoss/netflix-clone/src/components/header/styles/header.js",[],"/home/mantoss/netflix-clone/src/containers/Header.js",[],"/home/mantoss/netflix-clone/src/components/feature/index.js",[],"/home/mantoss/netflix-clone/src/components/feature/styles/feature.js",[],"/home/mantoss/netflix-clone/src/context/firebase.js",[],"/home/mantoss/netflix-clone/src/components/form/index.js",[],"/home/mantoss/netflix-clone/src/components/form/styles/form.js",[],"/home/mantoss/netflix-clone/src/pages/index.js",[],"/home/mantoss/netflix-clone/src/pages/Signin.js",[],"/home/mantoss/netflix-clone/src/pages/Signup.js",[],"/home/mantoss/netflix-clone/src/pages/BrowsePg.js",[],"/home/mantoss/netflix-clone/src/containers/Browse.js",["205","206"],"import React, { useState, useContext, useEffect } from 'react';\nimport Fuse from 'fuse.js';\nimport { Card, Header, Loading, Player } from '../components';\nimport * as ROUTES from '../constants/routes';\nimport { FirebaseContext } from '../context/firebase';\nimport { FooterContainer } from './Footer';\nimport { SelectProfileContainer } from './Profiles';\n\nexport function BrowseContainer({ slides }){\n    //initialize state\n    const [category, setCategory] = useState('series')\n    const [profile, setProfile] = useState({});\n    const [loading, setLoading] = useState(true);\n    const [searchTerm, setSearchTerm] = useState('');\n    const [slideRows, setSlideRows] = useState([]);\n\n    const { firebase } = useContext(FirebaseContext);\n\n    const user = {\n        displayName: \"Carl\",\n        photoURL: \"1\"\n    }\n\n    useEffect(() => {\n        setTimeout(() => {\n            setLoading(false)\n        }, 3000);\n    }, [user]);\n\n    useEffect(() => {\n        setSlideRows(slides[category]);\n    }, [slides, category]);\n\n    //live search implementation\n    useEffect(() => {\n        const fuse = new Fuse(slideRows, {keys: ['data.description', 'data.title', 'data.genre']});\n        const results = fuse.search(searchTerm).map(({ item }) => item);\n\n        if(slideRows.length > 0 && searchTerm.length > 3 && results.length > 0){\n            setSlideRows(results);\n        }else{\n            setSlideRows(slides[category]);\n        }\n    }, [searchTerm]);\n\n    return profile.displayName ? (\n          <>\n          {loading ? <Loading src={process.env.PUBLIC_URL + user.photoURL} /> : <Loading.ReleaseBody />}\n          <Header src=\"joker1\" dontShowOnSmallViewPort>\n              <Header.Frame>\n                  <Header.Group>\n                      <Header.Logo to={ROUTES.BROWSE} src={process.env.PUBLIC_URL + \"/images/misc/logo.svg\"} alt=\"Netflix\"/>\n                      <Header.Link \n                          active={category === 'series' ? 'true' : 'false'} \n                          onClick={() => setCategory('series')}\n                       >\n                            Series\n                       </Header.Link>\n\n                       <Header.Link \n                          active={category === 'films' ? 'true' : 'false'} \n                          onClick={() => setCategory('films')}\n                       >\n                            Films\n                       </Header.Link>\n                  </Header.Group>\n                  <Header.Group>\n                      <Header.Search searchTerm={searchTerm} setSearchTerm={setSearchTerm} />\n                      <Header.Profile>\n                          <Header.Picture src={user.photoURL} />\n                          <Header.Dropdown>\n                              <Header.Group>\n                                <Header.Picture src={user.photoURL} />\n                                <Header.Link>{user.displayName}</Header.Link>\n                              </Header.Group>\n\n                              <Header.Group>\n                                  <Header.Link onClick={() => firebase.auth().signOut()}>\n                                      Sign Out\n                                  </Header.Link>\n                              </Header.Group>\n                          </Header.Dropdown>\n                      </Header.Profile>\n                  </Header.Group>\n              </Header.Frame>\n\n              <Header.Feature>\n                  <Header.FeatureCallOut>Watch Joker now!</Header.FeatureCallOut>\n                  <Header.Text>\n                    Forever alone in a crowd, failed comedian Arthur Fleck seeks connection as he walks the streets of Gotham\n                    City. Arthur wears two masks -- the one he paints for his day job as a clown, and the guise he projects in a\n                    futile attempt to feel like he's part of the world around him.\n                  </Header.Text>\n              </Header.Feature>\n              <Header.PlayButton>Play</Header.PlayButton>\n          </Header>\n\n          <Card.Group>\n              {slideRows.map((slideItem) => (\n                  <Card key={`${category}-${slideItem.title.toLowerCase()}`}>\n                      <Card.Title>{slideItem.title}</Card.Title>\n                      <Card.Entities>\n                          {slideItem.data.map((item) => (\n                              <Card.Item key={item.docId} item={item}>\n                                  <Card.Image src={process.env.PUBLIC_URL + `/images/${category}/${item.genre}/${item.slug}/small.jpg`} />\n                                  <Card.Meta>\n                                      <Card.SubTitle>{item.title}</Card.SubTitle>\n                                      <Card.Text>{item.description}</Card.Text>\n                                  </Card.Meta>\n                              </Card.Item>\n                          